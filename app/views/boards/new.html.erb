<section id="main-content">
  <section class="wrapper">
  <%= form_for [current_user, @board], html: { method: :post } do |f| %>
    <p>
      <%= f.label :title %><br>
      <%= f.text_field :title %>
    </p>
    <p>
      <%= f.label :topic %><br>
      <%= f.text_area :topic %>
    </p>
      <%= f.submit %>
    </p>
    <% end %>
  </section>
</section>